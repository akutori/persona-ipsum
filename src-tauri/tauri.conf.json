{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "windows":{
      "wix": {
        "language": "ja-JP"
      }
    },
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "persona-ipsum",
  "mainBinaryName": "persona-ipsum",
  "version": "0.0.3",
  "identifier": "com.akutori.persona-ipsum",
  "plugins": {},
  "app": {
    "security": {
      "csp": "default-src 'self'; script-src 'self'; style-src 'self'"
    },
    "windows": [
      {
        "title": "ぺるそなIpsum",
        "width": 800,
        "height": 600,
        "useHttpsScheme": true
      }
    ]
  }
}